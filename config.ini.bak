[twitter]
; The Twitter account used for fetching the tweets, requires login.
login = LOGIN_USERNAME
; The Twitter account you want to fetch tweets from.
target = TARGET_USERNAME

[discord]
webhook_url = DISCORD_WEBHOOK_URL

[webhook]
; Set the interval time for checking if a new tweet was made by the user in seconds. Recommended to not set this below 60 seconds.
interval = 60

[webhook_message]
; All of these can be "yes" or "no"
; Include all images from a tweet.
all_images = no
; Include replies to be fetched.
replies = no
; Include quotes/quote retweets to be fetched.
quotes = no
; Includes all images from quote/reply tweets.
sub_images = no
